   "source": [
    "\n",
    "def feature(x):\n",
    "        if x in ('dec','jan','feb'):\n",
    "             return 'winter'\n",
    "        elif x in ('mar','apr','may'):\n",
    "             return 'summer'\n",
    "        elif x in ('jun','jul','aug'):\n",
    "             return 'monsoon'\n",
    "        else:\n",
    "             return 'autumn'\n",
    "df['season']=(df['month']).apply(feature)\n",
    "df"
   ]
