    "def converting(x):\n",
    "    for i in x:\n",
    "        if i in ('age','day','duration','campaign','pdays','previous'):\n",
    "            df[i]=df[i].astype(int)\n",
    "        elif i == 'balance':\n",
    "            df[i]=df[i].astype(float)\n",
    "converting(df.columns)\n",
    "    \n",
    "    \n"
