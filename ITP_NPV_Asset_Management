    "def covert(a):\n",
    "    if a < 5000000:\n",
    "        return 'typeA'\n",
    "    elif a>5000000 and a<7000000:\n",
    "        return 'typeB'\n",
    "    elif a>7000000:\n",
    "        return 'TypeC'\n",
    "    \n",
    "master_dataframe['rasied_amount_usd']=master_dataframe['raised_amount_usd'].apply(covert)\n",
    "master_dataframe"
   ]
  }
